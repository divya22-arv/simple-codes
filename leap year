import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    
	    Scanner in =new Scanner(System.in);
	    int table=0;
		System.out.println("enter the YEAR");
		int n=in.nextInt();
		if((n%4==0) && (n%100!=0||n%400==0))
		System.out.println("its a leapyear" +n);
		else
		
			System.out.println("itsnot  a leapyear" +n);

	}
}
