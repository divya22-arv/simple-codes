public class Main
{
	public static void main(String[] args)
	{
	  int a=0;
	  int b=1;  
	  int c=0;
	  int n=10;
	      	System.out.println("fibonacci series :");
	      	System.out.println(a);
	      	System.out.println(b);

  for(int i=3;i<n;i++)
  {
    c=a+b;
 System.out.println(c);
a=b;
b=c;
}
}
}
