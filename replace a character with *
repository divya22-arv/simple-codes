import java.util.*;
public class Main
{
	public static void main(String[] args)
	{
	    Scanner in=new Scanner(System.in);
	    
	    char k;
	    int j=0;
	    String M="";
	    System.out.println("Enter a string");
		String c=in.next();
		String s=c.toLowerCase(); 

		for(int i=0;i<s.length();i++)
	{
        k=s.charAt(i);
	
    if(k=='e')
    {
    M+='*';
    }
    else
    {
    M+=k;
	}
	}
  System.out.println("new string " + M);
	
	}
}
