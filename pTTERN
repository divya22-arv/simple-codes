import java.util.*;
public class Main
{
	public static void main(String[] args)
	{
	    Scanner in=new Scanner(System.in);
	    char k;
	    System.out.println("Enter a string");
		String c=in.next();
	System.out.println("the pattern is");

		for(int i =0;i<c.length();i++)
		{
	    k=c.charAt(i);
		System.out.println(k);
		}
	}
}
