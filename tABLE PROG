import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    
	    Scanner in =new Scanner(System.in);
	    int table=0;
		System.out.println("enter the numbers");
		int n=in.nextInt();
			System.out.println("table is" );
		for(int i=1;i<11;i++){
		table=n*i;
		System.out.println(  n + "*" + i + "=" +table);
		}
		
		
	
	}
}
