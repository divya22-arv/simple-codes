import java.util.*;
public class Main
{
	public static void main(String[] args)
	{
	    Scanner in=new Scanner(System.in);
	    
	    char k;
	    int j=0;
	    System.out.println("Enter a string");
		String c=in.next();
		for(int i=0;i<c.length();i++)
	{
        
     k=c.charAt(i);
    if(k=='e'|| k=='i' || k=='a' || k=='o'|| k=='u')
     	j=j+1;
		    
		}
     	
	System.out.println("number of vowels " + j);
	}
}
