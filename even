import java.util.*;
public class Main
{
	public static void main(String[] args) {
	    
	    Scanner in =new Scanner(System.in);
	    
		System.out.println("enter the numbers");
		int n=in.nextInt();
		if(n%2==0)
		
		System.out.println("number is even" +n);
		else
		System.out.println("number is odd" +n);
		
	}
}
