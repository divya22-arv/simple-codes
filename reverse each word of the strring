import java.util.*;
public class Main
{
	public static void main(String[] args)
	{
	    Scanner in=new Scanner(System.in);
String st1=" ",st2=" ";
char chr;
char ch1;
int p;
System.out.println("enter the string");
String st=in.nextLine();
st=st+" ";
for(int i =0;i<st.length();i++)
{
    chr=st.charAt(i);
    if(chr==' ')
    {
        st2=st2+' ' +st1;
        st1=" ";
}
else
st1=chr+st1;
}
System.out.println(st2);
}
}
